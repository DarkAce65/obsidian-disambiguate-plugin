{
  "name": "obsidian-disambiguate-plugin",
  "version": "1.0.0",
  "description": "This obsidian plugin provides a supercharged experience for unresolved links.",
  "main": "main.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "lint": "prettier --list-different \"*.config.{js,mjs,ts}\" \"tsconfig.json\" \"src/**/*.{js,ts}\" && eslint \"*.config.{js,mjs,ts}\" \"src/**/*.{js,ts}\"",
    "eslint:fix": "eslint --fix \"*.config.{js,mjs,ts}\" \"src/**/*.{js,ts}\"",
    "prettier:write": "prettier --write \"*.config.{js,mjs,ts}\" \"tsconfig.json\" \"src/**/*.{js,ts}\"",
    "dev": "node esbuild.config.mjs",
    "build": "tsc --noEmit --skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --max-warnings=0",
    "*.{js,ts}": "prettier --list-different"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "obsidian": "latest",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "monkey-around": "^3.0.0"
  }
}
